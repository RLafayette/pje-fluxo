        </transition>
        <transition to="(RPP) Sinalizar reativação" name="Reativar processo"/>
        <transition to="(RPP) Sinalizar manter arquivado" name="Manter processo arquivado"/>
    </task-node>
    <node name="(RPP) Sinalizar reativação">
