        <transition to="(VCiv) Possui meio Diário?" name="(VCiv) Possui meio Diário?"/>
    </fork>
    <decision expression="#{comunicacaoProcessualAction.getExpedientesEdital().size() &gt; 0 ? '(VCiv) Lançar movimento Expedição Outros documentos - Edital' : '(VCiv) Junção em Edital ou Diário'}" name="(VCiv) Possui meio Edital?">
        <transition to="(VCiv) Lançar movimento Expedição Outros documentos - Edital" name="(VCiv) Lançar movimento Expedição Outros documentos - Edital"/>
        <transition to="(VCiv) Junção em Edital ou Diário" name="(VCiv) Junção em Edital ou Diário"/>
    </decision>
    <decision expression="#{comunicacaoProcessualAction.getExpedientesDiario().size() &gt; 0 ? '(VCiv) Lançar movimento Expedição Outros documentos - Diário' : '(VCiv) Junção em Edital ou Diário'}" name="(VCiv) Possui meio Diário?">
        <transition to="(VCiv) Lançar movimento Expedição Outros documentos - Diário" name="(VCiv) Lançar movimento Expedição Outros documentos - Diário"/>
        <transition to="(VCiv) Junção em Edital ou Diário" name="(VCiv) Junção em Edital ou Diário"/>
