    <node name="(VCiv) Lançar movimento Expedição Outros documentos - Edital">
        <transition to="(VCiv) Publicar documento no DJEN" name="(VCiv) Publicar documento no DJEN"/>
        <event type="node-enter">
            <action expression="#{comunicacaoProcessualAction.enviarExpedientesLancarMovimentos('D', 'processoExpedienteAtual','#{preencherMovimento.deCodigo(60).associarAoDocumento(processoExpedienteAtual.getProcessoDocumento()).comComplementoDeCodigo(4).doTipoDominio().preencherComElementoDeCodigo(80).lancarMovimento()}')}"/>
        </event>
    </node>
    <node name="(VCiv) Lançar movimento Expedição Outros documentos - Diário">
        <transition to="(VCiv) Publicar documento no DJEN" name="(VCiv) Publicar documento no DJEN"/>
        <event type="node-enter">
            <action expression="#{comunicacaoProcessualAction.enviarExpedientesLancarMovimentos('P', 'processoExpedienteAtual','#{preencherMovimento.deCodigo(60).associarAoDocumento(processoExpedienteAtual.getProcessoDocumento()).comComplementoDeCodigo(4).doTipoDominio().preencherComElementoDeCodigo(80).lancarMovimento()}')}"/>
        </event>
    </node>
    <join name="(VCiv) Junção em Edital ou Diário">
        <transition to="(VCiv) Apagar variáveis do fluxo" name="(VCiv) Apagar variáveis do fluxo"/>
    </join>
    <node name="(VCiv) Apagar variáveis do fluxo">
        <transition to="(VCiv) Publicar documento no DJEN" name="(VCiv) Publicar documento no DJEN"/>
        <event type="node-enter">
            <action expression="#{tramitacaoProcessualService.apagaVariavel('idTaskInstanceTempEdital')}"/>
            <action expression="#{tramitacaoProcessualService.apagaVariavel('pg:tjrn:CitIntValidaParaAss')}"/>
        </event>
    </node>
    <node name="(VCiv) Publicar documento no DJEN">
        <transition to="Término" name="Término"/>
        <event type="node-enter">
            <action expression="#{processoJudicialService.incluirNovoFluxo(tramitacaoProcessualService.recuperaProcesso(), 'VCiv_PUB_DJEN', processInstance.contextInstance.variables)}"/>
        </event>
    </node>
    <end-state name="Término"/>
    <task-node end-tasks="true" name="Nó de Desvio - (VCiv) Preparação de documentos para o DJEN">
        <task name="Nó de Desvio - (VCiv) Preparação de documentos para o DJEN" swimlane="Nó de Desvio - (VCiv) Preparação de documentos para o DJEN" priority="3"/>
        <transition to="(VCiv) Publicar ou preparar documento para o DJEN" name="(VCiv) Publicar ou preparar documento para o DJEN"/>
        <transition to="Término" name="Término"/>
        <transition to="Nó de Desvio - (VCiv) Preparação de documentos para o DJEN" name="Nó de Desvio - (VCiv) Preparação de documentos para o DJEN"/>
    </task-node>  
